#!/bin/csh -f

set OSARCH   = `uname -s | gawk '{print tolower($0)}'`

switch ( $OSARCH )
    case irix*:
	set PROCTYPE = `hinv | gawk '/CPU/{print $3}'`
        switch ( $PROCTYPE )
            case R4???:
        	echo r4k
        	breaksw
            case R5???:
            case R10???:
        	echo r5k10k
        	breaksw
        endsw
	breaksw
    default:
	echo `uname -m`
	breaksw
endsw
