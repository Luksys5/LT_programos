#!/usr/bin/perl -w

use strict;

my @m = map {[split]} grep !/^\s*#|^\s*$/, <>;

## $\ = "\n";
## $, = ", ";
## for (@m) { print @$_ }

$[ = 1;
my $det = 
      $m[1][1] * $m[2][2] * $m[3][3]
    + $m[2][1] * $m[3][2] * $m[1][3]
    + $m[1][2] * $m[2][3] * $m[3][1]
    - $m[1][3] * $m[2][2] * $m[3][1]
    - $m[1][2] * $m[2][1] * $m[3][3]
    - $m[1][1] * $m[3][2] * $m[2][3];

printf "%10.6f\n", $det;

