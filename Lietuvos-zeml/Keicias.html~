<body>

<img id="Vilnius.jpg">
<script>
var vaizdas=1;

function keist_vaizda() 
{
  if(vaizdas<5){
    var p ="";
    p = "Vilnius" + vaizdas + ".jpg";
    document.getElementById("Vilnius").src= p;
    vaizdas++;
    if(vaizdas==5){
      vaizdas=1;
    }
  
  setTimeout("keist_vaizda()", 3000);
}

keist_vaizda();
</script>
 
</body>
