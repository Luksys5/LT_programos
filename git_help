git init <- makes you repozitory in working dir
git clone http:/// <--parsiuncia visa info is repoz
git checkout <-- pakeicia saka (branches)
git add <-- ideda faila
git commit <-- commitina faila
git push <-- nusiuncia i repozitorija faila(us)
git pull <-- parsiuncia is dabartines sakos faila(us)
git checkout --orphan <branchname> <-- sukuria nauja saka
git rm --cached -r . <- istrina darb dir
