        b.append(x[:-1]                                                                                                                                                                                                                                                    
                                                                                                                                                                                                                                            
'''                                                                                                                                                                                                                                                                             
for x in gridlist:
#        x=x.lower()                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                            
                os.system('wget ftp://ftp.wwpdb.org/pub/pdb/data/structures/divided/pdb/'+x[1:3]+'/pdb'+x[0:4]+'.ent.gz') #-r overwrite                                                                                                                                       

                os.system('gunzip -f '+'pdb'+x[0:4]+'.ent.gz')                                                                                                                                                                                                                  
                os.system('mv '+'pdb'+x[0:4]+'.ent'+' ./'+x[0:4]+'.pdb')

                os.system('mv '+x[0:4]+'.pdb'+' ./'+'visi')

os.system('/home/lukas/pdb/get_pdb_chains.pl Baigta.txt')
                                                                                                                                                                                                                                            
                                                                                                                                                                                                                

